{
  "openapi": "3.0.0",
  "info": {
    "title": "Futu Realtime API",
    "version": "1.0.0"
  },
  "paths": {
    "/api/push": {
      "post": {
        "operationId": "pushData",
        "summary": "Push realtime market data",
        "responses": {
          "200": { "description": "OK" }
        }
      }
    },
    "/api/latest": {
      "get": {
        "operationId": "getLatest",
        "summary": "Get latest pushed realtime data",
        "responses": {
          "200": {
            "description": "Latest data"
          }
        }
      }
    }
  }
}
